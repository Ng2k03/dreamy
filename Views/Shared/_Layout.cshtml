            }

        /* to place the button on top of images and make the button responsive */

        .img-wrapper {
            position: relative;
        }

        .img-responsive {
            width: 100%;
            height: auto;
        }

        .img-overlay {
            position: absolute;
            top: 0;
            bottom: 0;
            left: 0;
            right: 0;
            text-align: center;
        }

            .img-overlay:before {
                content: ' ';
                display: block;
                /* adjust 'height' to position overlay content vertically */
                height: 20%;
            }


        /*navbar*/
        body 
        {
       margin:0px;
       padding:0px;
       background: #F9F8FD;
       font-family: 'Lexend Exa', sans-serif;
}

            nav

        {
            display: flex;
            justify-content: space-around;
            animation: slide-in 1s ease-out;
                        position: absolute;
            top: 15px;
            left: 50%;
            transform: translateX(-50%);
            width: 100vw; 
            color: #635EF7;
            
}

 a 
 {
            color: #635EF7;
            
      text-decoration: none;
      text-transform: uppercase;
      transition: color 0.2s;
    }

ul {
  position: relative;
  display: flex;
  flex: 1 1 auto;
  margin: 0;
  padding: 0 30px;
  list-style-type: none;
  li:not(:last-child) {
    margin-right: 40px;
  }
  li {
    border: 2px solid transparent;
    border-radius: 5px;
    padding: 10px;
    transition: background 0.2s;
   
    ul {
      visibility: hidden;
      opacity: 0;
      position: absolute;
      display: block;
      margin: 12px -12px;
      padding: 0;
            background: #635EF7;
            border: 2px solid #635EF7;
            border-right: 2px solid #635EF7;
            border-bottom: 2px solid #635EF7;
      border-radius: 5px;
      transition: opacity 0.2s, visibility 0.2s;
      li {
        margin: -2px 0 0 -2px;
        width: calc(100% - 20px);
        line-height: 1.7;
        a {
          color: #635EF7;
        }
      }
    }
    &:hover {
      background: #E6E5FF;
            color: #635EF7;

      border: 2px solid white;
      border-right: 2px solid white;
      border-bottom: 2px solid white;
     
      a {
        color: #635EF7;
      }
      ul {
        visibility: visible;
        opacity: 1;
        box-shadow: 0px 3px 5px 2px #EBECF1;
        li {
          a {
            color: #F9F8FD;
          }
        }
      }

    }
  }
}

        .hovercolor:hover {
            color: #635EF7 !important;
        }

    .navigation{
            background-color: white;
            text-align: center;
            border-radius: 30px;

